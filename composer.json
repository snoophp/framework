{
    "name": "snoophp/framework",
	"description": "A light PHP framework inspired by Laravel",
	"keywords": [
		"framework",
		"snoophp",
		"sneppy"
	],
    "type": "library",
	"license": "MIT",
	"support": {
		"issues": "https://github.com/snoophp/framework/issues",
        "source": "https://github.com/snoophp/framework"
	},
    "authors": [
        {
            "name": "Sneppy",
            "email": "andrea.mecchia@mail.polimi.it"
        }
    ],
    "require": {
		"php": ">7.0"
	},
	"replace": {
		"snoophp/curl": "self.version",
		"snoophp/git": "self.version",
		"snoophp/http": "self.version",
		"snoophp/model": "self.version",
		"snoophp/utils": "self.version"
	},
	"autoload": {
		"files": [
			"src/SnooPHP/Utils/helpers.php"
		],
        "psr-4": {
            "SnooPHP\\": "src/SnooPHP/"
        }
    },
	"minimum-stability": "dev"
}
